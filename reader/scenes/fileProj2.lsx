                <DESCENDENT id="mainBody" />
                <DESCENDENT id="frontBody" /> 
            </DESCENDANTS>
        </NODE>
        
        <NODE id="mainBody">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="0" y="0.5" z="-1" />
            
            <DESCENDANTS>
                <DESCENDENT id="mainCyl" />
                <DESCENDENT id="wing0" />
                <DESCENDENT id="wing1" />
                <DESCENDENT id="wing2" />
                <!--<DESCENDENT id="wing3" />
                <DESCENDENT id="wing4" />-->
            </DESCENDANTS>
        </NODE>

        <NODE id="mainCyl">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="0" y="0" z="-1.5" />
            <SCALE sx="0.25" sy="0.25" sz="3" />
            <DESCENDANTS>
                <DESCENDENT id="cylinder1" />
            </DESCENDANTS>
        </NODE>

        <NODE id="wing0">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
           <TRANSLATION x="0.2" y="0" z="0.5" />
            <ROTATION axis="y" angle="-90" />
            <ROTATION axis="z" angle="90" />
            <SCALE sx="0.25" sy="0.5" sz="1" />
            <DESCENDANTS>
