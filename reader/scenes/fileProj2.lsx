        <NODE id="wing1">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="-0.2" y="0" z="0.5" />
            <ROTATION axis="y" angle="90" />
            <ROTATION axis="z" angle="90" />
            <SCALE sx="0.25" sy="0.5" sz="1" />
            <DESCENDANTS>
                <DESCENDENT id="wing" />
            </DESCENDANTS>
        </NODE>

        <NODE id="wing2">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="0.2" y="0" z="-1" />
            <ROTATION axis="y" angle="-90" />
            <ROTATION axis="z" angle="90" />
            <SCALE sx="0.25" sy="0.25" sz="0.5" />
            <DESCENDANTS>
                <DESCENDENT id="wing" />
            </DESCENDANTS>
        </NODE>

         <NODE id="wing3">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="-0.2" y="0" z="-1" />
            <ROTATION axis="y" angle="-90" />
            <SCALE sx="0.5" sy="0.1" sz="0.1" />
            <DESCENDANTS>
                <DESCENDENT id="wing" />
            </DESCENDANTS>
        </NODE>

        <NODE id="wing4">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="0" y="0.25" z="-1.25" />
            <ROTATION axis="x" angle="-90" />
            <ROTATION axis="z" angle="-90" />
            <SCALE sx="0.5" sy="0.1" sz="0.1" />
            <DESCENDANTS>
                <DESCENDENT id="wing" />
            </DESCENDANTS>
        </NODE>


        <NODE id="wing">
