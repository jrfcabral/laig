            </DESCENDANTS>
        </NODE>



        <NODE id="testerino">
            <MATERIAL id="null" />
            <TEXTURE id="clear" />
            <ANIMATION id="test" />
           <!-- <ANIMATION id="testCirc"/>-->
           <ROTATION axis="z" angle="180" />
            <DESCENDANTS>
                <DESCENDENT id="triangle1" />
                <DESCENDENT id="testerinono" />
            </DESCENDANTS>
        </NODE>

        <NODE id="testerinono">
            <MATERIAL id="null" />
            <TEXTURE id="clear" />
            <TRANSLATION x="0"  y="0" z="1" />
            <DESCENDANTS>
