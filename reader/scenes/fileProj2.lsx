            </DESCENDANTS>
        </NODE>

         <NODE id="landingGear2">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            <TRANSLATION x="-0.1" y="0" z="-1.5" />
            <DESCENDANTS>
                <DESCENDENT id="landingGear" />
            </DESCENDANTS>
        </NODE>

         <NODE id="landingGear">
            <MATERIAL id="null" />
            <TEXTURE id="null" />
            
            <DESCENDANTS>
